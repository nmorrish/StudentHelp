@model Student
@{
    ViewData["Title"] = "Delete";
}
<div class="d-flex justify-content-center">
    <h1>Delete @Model.FirstName @Model.LastName ?</h1>
</div>

<div class="d-flex justify-content-center">
    <form asp-action="Delete" asp-route-id="@Model.StudentId">
        <button class="btn btn-danger"> Yes, Delete</button>
    </form>
    <a class="btn btn-success" asp-action="Index" asp-controller="Student"> No, Go Back</a>
</div>
